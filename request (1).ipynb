    "import requests\n",
    "\n",
    "img_files = []\n",
    "for img in glob.glob('data_testing/*'):\n",
    "    img_files.append((\"image\", open(img, 'rb')))\n",
    "\n",
    "    \n",
    "url = 'http://127.0.0.1:5000/predict'\n",
    "r = requests.post(url, files=img_files)"
